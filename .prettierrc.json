{
  "plugins": [
    "./node_modules/prettier-plugin-astro/dist/index.js",
    "./node_modules/prettier-plugin-tailwindcss/dist/index.mjs"
  ],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true
}
